# Snowflake Connection Configuration
# Copy this file to .env and fill in your actual credentials
# NEVER commit .env to git!

SNOWFLAKE_ACCOUNT=your_account_identifier_here
SNOWFLAKE_USER=your_username_here
SNOWFLAKE_PASSWORD=your_password_here
SNOWFLAKE_ROLE=MIGRATION_ADMIN
SNOWFLAKE_WAREHOUSE=MIGRATION_WH
SNOWFLAKE_DATABASE=ORACLE_MIGRATION
SNOWFLAKE_SCHEMA=INTERMEDIATE

# Example:
# SNOWFLAKE_ACCOUNT=abc12345.east-us-2.azure
# SNOWFLAKE_USER=myuser
# SNOWFLAKE_PASSWORD=mypassword123
